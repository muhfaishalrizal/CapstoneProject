<?xml version="1.0" encoding="utf-8"?><androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    app:cardCornerRadius="20dp"    android:elevation="10dp"    android:layout_margin="6dp">    <androidx.constraintlayout.widget.ConstraintLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_margin="10dp">        <ImageView            android:id="@+id/ivEventList"            android:layout_width="80dp"            android:layout_height="120dp"            android:src="@drawable/sample_event"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent" />        <TextView            android:id="@+id/tvNameEvent"            style="@style/AppTheme.TextTitle"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginStart="16dp"            android:layout_marginTop="8dp"            android:text="Judul Event"            android:textStyle="bold"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toEndOf="@+id/ivEventList"            app:layout_constraintTop_toTopOf="@+id/ivEventList" />        <TextView            android:id="@+id/tvDate"            style="@style/AppTheme.TextMain"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="2022-01-12"            app:layout_constraintStart_toStartOf="@+id/tvNameEvent"            app:layout_constraintTop_toBottomOf="@+id/tvNameEvent" />        <TextView            android:id="@+id/tvDesc"            style="@style/AppTheme.TextMain"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginEnd="8dp"            android:maxLines="3"            android:text="Desc"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="@+id/tvDate"            app:layout_constraintTop_toBottomOf="@+id/tvDate" />    </androidx.constraintlayout.widget.ConstraintLayout></androidx.cardview.widget.CardView>